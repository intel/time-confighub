<cnc-config xmlns="urn:ieee:std:802.1Q:yang:ieee802-dot1q-cnc-config">
  <domain>
    <domain-id>chronos-domain</domain-id>
    <cnc-enabled>true</cnc-enabled>
    <cuc>
      <cuc-id>SAMPLE-CONFIG</cuc-id>
      <!-- Stream 1: TA stream mapped to PCP 7 with offset 1000000 -->
      <stream>
        <stream-id>aa-bb-cc-dd-ee-01:00-01</stream-id>
        <talker>
          <end-station-interfaces>
            <mac-address>AA-BB-CC-DD-EE-01</mac-address>
            <interface-name></interface-name>
          </end-station-interfaces>
          <data-frame-specification>
            <index>1</index>
            <ipv4-tuple>
              <source-ip-address>192.168.1.10</source-ip-address>
              <destination-ip-address>239.255.1.1</destination-ip-address>
              <dscp>0</dscp>
              <protocol>17</protocol>
              <source-port>5000</source-port>
              <destination-port>5001</destination-port>
            </ipv4-tuple>
          </data-frame-specification>
          <traffic-specification>
            <interval>
              <numerator>1000000</numerator>
              <denominator>1000000000</denominator>
            </interval>
            <max-frames-per-interval>1</max-frames-per-interval>
            <max-frame-size>1500</max-frame-size>
            <transmission-selection>0</transmission-selection>
            <time-aware>
              <earliest-transmit-offset>0</earliest-transmit-offset>
              <latest-transmit-offset>0</latest-transmit-offset>
            </time-aware>
          </traffic-specification>
          <interface-configuration>
            <interface-list>
              <mac-address>AA-BB-CC-DD-EE-01</mac-address>
              <interface-name>enp2s0</interface-name>
              <config-list>
                <index>1</index>
                <ieee802-mac-addresses>
                  <source-mac-address>AA-BB-CC-DD-EE-01</source-mac-address>
                  <destination-mac-address>01-00-5E-7F-01-01</destination-mac-address>
                </ieee802-mac-addresses>
              </config-list>
              <config-list>
                <index>2</index>
                <ieee802-vlan-tag>
                  <priority-code-point>7</priority-code-point>
                  <vlan-id>100</vlan-id>
                </ieee802-vlan-tag>
              </config-list>
              <config-list>
                <index>3</index>
                <ipv4-tuple>
                  <source-ip-address>192.168.1.10</source-ip-address>
                  <destination-ip-address>239.255.1.1</destination-ip-address>
                  <dscp>0</dscp>
                  <protocol>17</protocol>
                  <source-port>5000</source-port>
                  <destination-port>5001</destination-port>
                </ipv4-tuple>
              </config-list>
              <config-list>
                <index>4</index>
                <time-aware-offset>1</time-aware-offset>
              </config-list>
            </interface-list>
          </interface-configuration>
        </talker>
        <listener>
          <index>1</index>
          <end-station-interfaces>
            <mac-address>AA-BB-CC-DD-EE-02</mac-address>
            <interface-name></interface-name>
          </end-station-interfaces>
        </listener>
      </stream>
      <!-- Stream 2: TA stream mapped to PCP 6 with offset 2000000 -->
      <stream>
        <stream-id>aa-bb-cc-dd-ee-01:00-02</stream-id>
        <talker>
          <end-station-interfaces>
            <mac-address>AA-BB-CC-DD-EE-01</mac-address>
            <interface-name></interface-name>
          </end-station-interfaces>
          <data-frame-specification>
            <index>1</index>
            <ipv4-tuple>
              <source-ip-address>192.168.1.10</source-ip-address>
              <destination-ip-address>239.255.1.2</destination-ip-address>
              <dscp>0</dscp>
              <protocol>17</protocol>
              <source-port>5000</source-port>
              <destination-port>5002</destination-port>
            </ipv4-tuple>
          </data-frame-specification>
          <traffic-specification>
            <interval>
              <numerator>1000000</numerator>
              <denominator>1000000000</denominator>
            </interval>
            <max-frames-per-interval>1</max-frames-per-interval>
            <max-frame-size>1500</max-frame-size>
            <transmission-selection>0</transmission-selection>
            <time-aware>
              <earliest-transmit-offset>0</earliest-transmit-offset>
              <latest-transmit-offset>0</latest-transmit-offset>
            </time-aware>
          </traffic-specification>
          <interface-configuration>
            <interface-list>
              <mac-address>AA-BB-CC-DD-EE-01</mac-address>
              <interface-name>enp2s0</interface-name>
              <config-list>
                <index>1</index>
                <ieee802-mac-addresses>
                  <source-mac-address>AA-BB-CC-DD-EE-01</source-mac-address>
                  <destination-mac-address>01-00-5E-7F-01-02</destination-mac-address>
                </ieee802-mac-addresses>
              </config-list>
              <config-list>
                <index>2</index>
                <ieee802-vlan-tag>
                  <priority-code-point>6</priority-code-point>
                  <vlan-id>200</vlan-id>
                </ieee802-vlan-tag>
              </config-list>
              <config-list>
                <index>3</index>
                <ipv4-tuple>
                  <source-ip-address>192.168.1.10</source-ip-address>
                  <destination-ip-address>239.255.1.2</destination-ip-address>
                  <dscp>0</dscp>
                  <protocol>17</protocol>
                  <source-port>5000</source-port>
                  <destination-port>5002</destination-port>
                </ipv4-tuple>
              </config-list>
              <config-list>
                <index>4</index>
                <time-aware-offset>1000000</time-aware-offset>
              </config-list>
            </interface-list>
          </interface-configuration>
        </talker>
        <listener>
          <index>1</index>
          <end-station-interfaces>
            <mac-address>AA-BB-CC-DD-EE-03</mac-address>
            <interface-name></interface-name>
          </end-station-interfaces>
        </listener>
      </stream>
      <!-- Stream 3: Non-TA VLAN stream with PCP 4 -->
      <stream>
        <stream-id>aa-bb-cc-dd-ee-02:00-03</stream-id>
        <talker>
          <end-station-interfaces>
            <mac-address>AA-BB-CC-DD-EE-02</mac-address>
            <interface-name></interface-name>
          </end-station-interfaces>
          <data-frame-specification>
            <index>1</index>
            <ipv4-tuple>
              <source-ip-address>192.168.1.20</source-ip-address>
              <destination-ip-address>239.255.1.3</destination-ip-address>
              <dscp>0</dscp>
              <protocol>17</protocol>
              <source-port>6000</source-port>
              <destination-port>6003</destination-port>
            </ipv4-tuple>
          </data-frame-specification>
          <traffic-specification>
            <interval>
              <numerator>5000000</numerator>
              <denominator>1000000000</denominator>
            </interval>
            <max-frames-per-interval>1</max-frames-per-interval>
            <max-frame-size>1200</max-frame-size>
            <transmission-selection>0</transmission-selection>
          </traffic-specification>
          <interface-configuration>
            <interface-list>
              <mac-address>AA-BB-CC-DD-EE-02</mac-address>
              <interface-name>enp2s0</interface-name>
              <config-list>
                <index>1</index>
                <ieee802-mac-addresses>
                  <source-mac-address>AA-BB-CC-DD-EE-02</source-mac-address>
                  <destination-mac-address>01-00-5E-7F-01-03</destination-mac-address>
                </ieee802-mac-addresses>
              </config-list>
              <config-list>
                <index>2</index>
                <ieee802-vlan-tag>
                  <priority-code-point>4</priority-code-point>
                  <vlan-id>300</vlan-id>
                </ieee802-vlan-tag>
              </config-list>
              <config-list>
                <index>3</index>
                <ipv4-tuple>
                  <source-ip-address>192.168.1.20</source-ip-address>
                  <destination-ip-address>239.255.1.3</destination-ip-address>
                  <dscp>0</dscp>
                  <protocol>17</protocol>
                  <source-port>6000</source-port>
                  <destination-port>6003</destination-port>
                </ipv4-tuple>
              </config-list>
            </interface-list>
          </interface-configuration>
        </talker>
        <listener>
          <index>1</index>
          <end-station-interfaces>
            <mac-address>AA-BB-CC-DD-EE-04</mac-address>
            <interface-name></interface-name>
          </end-station-interfaces>
        </listener>
      </stream>
      <!-- Stream 4: Non-TA Non-VLAN stream -->
      <stream>
        <stream-id>aa-bb-cc-dd-ee-03:00-04</stream-id>
        <talker>
          <end-station-interfaces>
            <mac-address>AA-BB-CC-DD-EE-03</mac-address>
            <interface-name></interface-name>
          </end-station-interfaces>
          <data-frame-specification>
            <index>1</index>
            <ipv4-tuple>
              <source-ip-address>192.168.1.30</source-ip-address>
              <destination-ip-address>239.255.1.4</destination-ip-address>
              <dscp>0</dscp>
              <protocol>17</protocol>
              <source-port>7000</source-port>
              <destination-port>7004</destination-port>
            </ipv4-tuple>
          </data-frame-specification>
          <traffic-specification>
            <interval>
              <numerator>10000000</numerator>
              <denominator>1000000000</denominator>
            </interval>
            <max-frames-per-interval>1</max-frames-per-interval>
            <max-frame-size>1000</max-frame-size>
            <transmission-selection>0</transmission-selection>
          </traffic-specification>
          <interface-configuration>
            <interface-list>
              <mac-address>AA-BB-CC-DD-EE-03</mac-address>
              <interface-name>enp2s0</interface-name>
              <config-list>
                <index>1</index>
                <ieee802-mac-addresses>
                  <source-mac-address>AA-BB-CC-DD-EE-03</source-mac-address>
                  <destination-mac-address>01-00-5E-7F-01-04</destination-mac-address>
                </ieee802-mac-addresses>
              </config-list>
              <config-list>
                <index>2</index>
                <ipv4-tuple>
                  <source-ip-address>192.168.1.30</source-ip-address>
                  <destination-ip-address>239.255.1.4</destination-ip-address>
                  <dscp>0</dscp>
                  <protocol>17</protocol>
                  <source-port>7000</source-port>
                  <destination-port>7004</destination-port>
                </ipv4-tuple>
              </config-list>
            </interface-list>
          </interface-configuration>
        </talker>
        <listener>
          <index>1</index>
          <end-station-interfaces>
            <mac-address>AA-BB-CC-DD-EE-05</mac-address>
            <interface-name></interface-name>
          </end-station-interfaces>
        </listener>
      </stream>
    </cuc>
  </domain>
</cnc-config>
<interfaces xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces">
  <interface>
    <name>enp2s0</name>
    <type xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
    <enabled>true</enabled>
    <gate-parameter-table xmlns="urn:ieee:std:802.1Q:yang:ieee802-dot1dc-sched-if" xmlns:sched-if="urn:ieee:std:802.1Q:yang:ieee802-dot1dc-sched-if" xmlns:sched="urn:ieee:std:802.1Q:yang:ieee802-dot1q-sched">
      <gate-enabled>true</gate-enabled>
      <admin-control-list>
        <!-- Entry 0: Open gates for PCP 0,1 before first TA window -->
        <gate-control-entry>
          <index>0</index>
          <operation-name>sched:set-gate-states</operation-name>
          <gate-states-value>8</gate-states-value>
          <time-interval-value>1000000</time-interval-value>
        </gate-control-entry>
        <!-- Entry 1: Open gate for PCP 3 (TA stream 1 at offset 1000000) -->
        <gate-control-entry>
          <index>1</index>
          <operation-name>sched:set-gate-states</operation-name>
          <gate-states-value>4</gate-states-value>
          <time-interval-value>1000000</time-interval-value>
        </gate-control-entry>
        <!-- Entry 2: Open gates for PCP 0,1 between TA windows -->
        <gate-control-entry>
          <index>2</index>
          <operation-name>sched:set-gate-states</operation-name>
          <gate-states-value>2</gate-states-value>
          <time-interval-value>1000000</time-interval-value>
        </gate-control-entry>
        <!-- Entry 3: Open gate for PCP 2 (TA stream 2 at offset 2000000) -->
        <gate-control-entry>
          <index>3</index>
          <operation-name>sched:set-gate-states</operation-name>
          <gate-states-value>1</gate-states-value>
          <time-interval-value>1000000</time-interval-value>
        </gate-control-entry>
      </admin-control-list>
      <admin-cycle-time>
        <numerator>10000000</numerator>
        <denominator>1000000000</denominator>
      </admin-cycle-time>
      <admin-base-time>
        <seconds>0</seconds>
        <nanoseconds>0</nanoseconds>
      </admin-base-time>
      <supported-list-max>256</supported-list-max>
      <supported-interval-max>1048568</supported-interval-max>
      <supported-cycle-max>
        <numerator>4294967295</numerator>
        <denominator>1000000000</denominator>
      </supported-cycle-max>
    </gate-parameter-table>
    <cbsa xmlns="urn:ieee:std:802.1Q:yang:ieee802-dot1dc-cbsa-if">
      <cbsa-parameter-table>
        <traffic-class>3</traffic-class>
        <admin-idle-slope>5000000</admin-idle-slope>
      </cbsa-parameter-table>
    </cbsa>
  </interface>
</interfaces>